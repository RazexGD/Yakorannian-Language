fetch('../data/yakorannian_words.json')
    .then(response => response.json())
    .then(data => {
        data.words.forEach(word => {
            document.body.innerHTML += 
                <div class="word-card">
                    <h3>${word.word}</h3>
                    <p>Перевод: ${word.translation}</p>
                </div>
            ;
        });
    });
